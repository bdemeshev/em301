hs2_all <- c(10, 20, 30, 40)



set.seed(var_no)
k <- sample(k_all, 1)
hs2 <- sample(hs2_all, 1)

n <- 50 + k

RSS <- (n - k) * hs2
ESS <- 100
TSS <- RSS + ESS

wrong <- RSS / (n - 2)
wrong0 <- round(wrong, 1)
@



